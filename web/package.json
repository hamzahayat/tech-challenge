    "bfj": "6.1.1",
    "chalk": "2.4.1",
    "chart.js": "^2.7.3",
    "classnames": "^2.2.6",
    "connected-react-router": "^5.0.1",
    "history": "^4.7.2",
    "identity-obj-proxy": "3.0.0",
    "moment": "^2.23.0",
    "node-fetch": "2.0.0",
    "normalizr": "^3.3.0",
    "portable-fetch": "^3.0.0",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "rc-progress": "^2.2.6",
    "rc-steps": "^3.3.0",
    "react": "^16.8.3",
    "react-app-polyfill": "^0.1.3",
    "react-chartjs-2": "^2.7.4",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.3",
    "react-flip-toolkit": "^6.3.0",
    "react-photo-gallery": "^6.3.2",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
