    "sass-loader": "7.1.0",
    "sass-mq": "^5.0.0",
    "serve": "^10.1.1",
    "style-loader": "0.23.0",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "dependencies": {
